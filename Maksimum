def maxelements(seq):
    ''' Return list of position(s) of largest element '''
    max_indices = []
    if seq:
        max_val = seq[0]
        for i,val in ((i,val) for i,val in enumerate(seq) if val >= max_val):
            if val == max_val:
                max_indices.append(i)
            else:
                max_val = val
                max_indices = [i]

    return max_indices
    
def czebyszew(x,y)
    max=0
    i=0
    while i<len(x)
        if fabs(x[i]-y[i]) >= max
            max=fabs(x[i]-y[i])
    return max
    
    
